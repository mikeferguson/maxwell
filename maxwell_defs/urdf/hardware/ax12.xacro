        <origin xyz="0.0 0.0 ${-0.0501/2+0.0116}" rpy="0 0 0" />
        <geometry>
          <box size="0.032 0.040 0.0501"/>
        </geometry>
      </collision>
    </link>
  </macro>

  <macro name="ax12_side_revolute" params="name parent llimit ulimit vlimit color *origin" >
    <joint name="${name}_joint" type="revolute">
      <insert_block name="origin" />
      <axis xyz="0 0 1"/>
      <limit effort="30" velocity="${vlimit}" lower="${llimit}" upper="${ulimit}" />
      <parent link="${parent}"/>
      <child link="${name}_link" />
    </joint>
    <link name="${name}_link">
      <inertial>
        <mass value="0.00001" />
        <origin xyz="0 0 0" />
        <inertia ixx="1.0" ixy="0.0" ixz="0.0"
          iyy="1.0" iyz="0.0"
          izz="1.0" />
      </inertial>
      <visual>
        <origin xyz=" 0 0 0 " rpy="3.14 0 0" />
        <geometry>
          <mesh filename="package://maxwell_defs/meshes/F3.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="${color}"/>
      </visual>
      <collision>
        <origin xyz="0.0 0.0 0.0045" rpy="0 0 0" />
        <geometry>
          <box size="0.025 0.038 0.009"/>
        </geometry>
      </collision>
    </link>
  </macro>

